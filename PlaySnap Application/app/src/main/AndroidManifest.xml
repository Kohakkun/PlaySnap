<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/playsnaplogo"
        android:label="PlaySnap"
        android:roundIcon="@drawable/playsnaplogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.PlaySnapUI"
        tools:targetApi="31">
        <activity
            android:name=".BookmarkPageActivity"
            android:exported="false" />
        <activity
            android:name=".LikePageActivity"
            android:exported="false" />
        <activity
            android:name=".NavigationBar"
            android:exported="false" />
        <activity
            android:name=".HomePageActivity"
            android:exported="false" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>